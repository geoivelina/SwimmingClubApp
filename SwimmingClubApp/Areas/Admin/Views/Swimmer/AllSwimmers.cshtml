@model IEnumerable<SwimmerDetailsServiceModel>

@{
    ViewBag.Title = "All Approved Swimmers";
}

<h1 class="text-center">@ViewBag.Title</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Full Name</th>
            <th scope="col">Age</th>
            <th scope="col">Contact Person</th>
            <th scope="col">Relationship With Swimmer</th>
            <th scope="col">Phone Number</th>
            <th scope="col">Email</th>
            <th scope="col">Address</th>
            <th scope="col">Medical Datails</th>
            <th scope="col">Swimming Experience</th>
            <th scope="col">Squad</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var entity in Model)
        {
            <tr>
                <td scope="col">@entity.Id</td>
                <td scope="col">@entity.FullName</td>
                <td scope="col">@entity.Age</td>
                <td scope="col">@entity.ContactPersonName</td>
                <td scope="col">@entity.RelationshipToSwimmer</td>
                <td scope="col">@entity.PhoneNumber</td>
                <td scope="col">@entity.Email</td>
                <td scope="col">@entity.Address</td>
                <td scope="col">@entity.MedicalDatails</td>
                <td scope="col">@entity.SwimmingExperience</td>
                <td scope="col">@entity.SquadName</td>
                <td scope="col">
                    <a asp-area="Admin" asp-controller="Swimmer" asp-action="Disapprove" asp-route-id="@entity.Id" class="btn btn-sm btn-outline-secondary">Disapprove</a>
                </td>
            </tr>
        }
    </tbody>
</table>